[user]
	name = Albert Carter
	email = albert.r.carter@gmail.com
[core]
	autocrlf = false
	safecrlf = true
	editor = vim
	excludesfile = /Users/arcarter/.gitignore_global

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -pretty
  gs='git status '
  ga='git add '
  gb='git branch '
  gc='git commit'
  gd='git diff'
  go='git checkout '
  gk='gitk --all&'
  gx='gitx --all'

  got='git '
  get='git '
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
